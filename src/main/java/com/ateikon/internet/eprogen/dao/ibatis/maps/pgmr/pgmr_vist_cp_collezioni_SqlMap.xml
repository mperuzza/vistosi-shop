       </isNotEmpty>
       <isNotEmpty prepend="AND" property="cdvistv3">
            vist_articoli.cdvistv3 = #cdvistv3:VARCHAR#
       </isNotEmpty>
       <isNotEmpty prepend="AND" property="cdclas_a">
            vist_articoli.cdclas_a = #cdclas_a:VARCHAR#
       </isNotEmpty>
       <isNotEmpty prepend="AND" property="cdclas_aList">
            vist_articoli.cdclas_a IN
            <iterate close=")" conjunction="," open="(" property="cdclas_aList">
                #cdclas_aList[]#
            </iterate>
       </isNotEmpty>
       <isNotEmpty prepend="AND" property="fgweb">
             vist_articoli.fgweb = #fgweb:VARCHAR#
        AND  mis_reparto.fgweb = 'S'            
       </isNotEmpty>
       <isNotEmpty prepend="AND" property="cdrepa_escl_List">
            mis_reparto.cdrepa NOT IN
            <iterate close=")" conjunction="," open="(" property="cdrepa_escl_List">
                #cdrepa_escl_List[]#
            </iterate>
       </isNotEmpty>       
