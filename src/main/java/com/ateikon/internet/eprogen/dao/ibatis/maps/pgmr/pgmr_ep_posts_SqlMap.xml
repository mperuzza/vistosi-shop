    <resultMap class="com.ateikon.internet.eprogen.domain.pgmr.Ep_posts" id="techNews_ResultMap">
        <result column="tkpost" property="tkpost" jdbcType="NUMERIC" />
        <result column="post_date" property="postDate" jdbcType="TIMESTAMP" />
        <result column="fgpubblico" property="fgpubblico" jdbcType="VARCHAR" />
    </resultMap>    
    <select id="getTechNews" parameterClass="map" resultMap="techNews_ResultMap">
        SELECT  DISTINCT(posts.tkpost)
        , posts.post_date                     
        , posts.fgpubblico        
        , tags.cdvistelet               
          FROM pgmr.ep_posts    posts         
        , pgmr.ep_media_content media         
        , pgmr.ep_media_tags    tags          
        WHERE posts.tkpost = media.parent_id      
        AND media.tkmedia = tags.tkmedia        
        AND posts.fgpubblico = 'S'   
        AND posts.post_category = 6
        <isNotEmpty prepend="AND" property="postDate" >
            post_date <![CDATA[ <= ]]> #postDate:TIMESTAMP#
        </isNotEmpty>                   
        <isNotEmpty prepend="AND" property="cdvistfam">
            tags.cdvistfam = #cdvistfam:VARCHAR#                  
        </isNotEmpty>
        <isNotEmpty prepend="AND" property="cdvisttp">
            tags.cdvisttp = #cdvisttp:VARCHAR#                   
        </isNotEmpty>
        AND (
            tags.cdvistelet is null
            <isNotEmpty prepend="OR" property="cdvistelet">
                 tags.cdvistelet = #cdvistelet:VARCHAR#
            </isNotEmpty>        
        )
        ORDER BY tags.cdvistelet desc, post_date desc  
    </select>      
  
  
