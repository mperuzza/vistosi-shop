  <select id="getVoceSelezione" parameterClass="map" resultMap="voceSelezioneItemResultMap">
    SELECT voce_selezione.cdvoce,
		   voce_selezione.dsvoce,
          (SELECT nrposi FROM pgmr.articoli_linea
						WHERE articoli_linea.cdarticolo = art.cdarticolo
						  AND articoli_linea.cdcollezione = art.cdcollezione
						    ) as nr,
		  art.*
	 FROM pgmr.articoli art, pgmr.articoli_abbinati, pgmr.voce_selezione
 	   WHERE articoli_abbinati.cdvoce = voce_selezione.cdvoce
			 AND art.cdarticolo = articoli_abbinati.cdarticoloabb
			 AND art.cdlingua = voce_selezione.cdlingua
             AND (art.cdcollezione = #cdcollezione:INTEGER# OR art.cdcollezione IN (SELECT cdcollezioneabb FROM pgmr.collezioni_Abbinate WHERE cdcollezione = #cdcollezione:INTEGER#))
			 AND voce_selezione.cdlingua = #cdlingua:INTEGER#
			 AND articoli_abbinati.cdarticolo = #cdarticolo:VARCHAR#
	ORDER BY voce_selezione.nrposi, nr
  </select>
