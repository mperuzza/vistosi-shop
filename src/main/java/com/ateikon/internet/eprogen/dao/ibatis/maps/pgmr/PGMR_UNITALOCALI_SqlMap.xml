    <result column="FSELEG" jdbcType="VARCHAR" property="enteuniloc.fseleg" />
    <result column="FSPREF" jdbcType="VARCHAR" property="enteuniloc.fspref" />
    <result column="CDENTE" jdbcType="VARCHAR" property="enteuniloc.cdente" />
  </resultMap>


  <select id="searchUnitalocali" parameterClass="map" resultMap="searchUnitalocaleResult">
    SELECT u.*,
           eu.FSELEG,
           eu.FSPREF,
           eu.cdente
           <!--,
           p.dsprov,
           n.dsnazi,
           c.dscomu-->
      FROM pgmr.archclie ac,
           pgmr.archenti ae,
           pgmr.enteuniloc eu,
           pgmr.unitalocali u
         <!-- , pgmr.province p,
           pgmr.nazioni n,
           pgmr.comunita c-->
     WHERE ac.cdente = ae.cdente
       AND ae.cdente = eu.cdente
       AND eu.cdunil = u.cdunil
       <!--AND u.cdprov = p.cdprov(+)
       AND u.cdnazi = n.cdnazi(+)
       AND u.cdcomu = c.cdcomu(+)-->
     <isNotEmpty prepend="AND" property="cdunil">
           u.cdunil = #cdunil#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="tkclie">
           ac.tkclie = #tkclie#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="cdente">
           ac.cdente = #cdente#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="cdazie">
           u.cdazie = #cdazie#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="cddipa">
           u.cddipa = #cddipa#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="cdusul">
           eu.cdusul= #cdusul#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="fseleg">
           eu.fseleg= #fseleg#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="dtfval">
           ( u.dtfval is null OR  u.dtfval <![CDATA[ > #dtfval:TIMESTAMP# ]]> )
     </isNotEmpty>
     ORDER BY u.dsunil ASC, eu.FSELEG
  </select>

  <select id="decodeUnitalocali" parameterClass="map" resultMap="searchUnitalocaleResult">
    SELECT u.*,
           eu.FSELEG,
           eu.FSPREF,
           eu.cdente,
           p.dsprov,
           n.dsnazi,
           c.dscomu
      FROM pgmr.enteuniloc eu,
           pgmr.unitalocali u,
           pgmr.province p,
           pgmr.nazioni n,
           pgmr.comunita c
     WHERE eu.cdunil = u.cdunil
       AND u.cdprov = p.cdprov(+)
       AND u.cdnazi = n.cdnazi(+)
       AND u.cdcomu = c.cdcomu(+)
     <isNotEmpty prepend="AND" property="cdunil">
           u.cdunil = #cdunil#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="cdazie">
           u.cdazie = #cdazie#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="cddipa">
           u.cddipa = #cddipa#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="cdusul">
           eu.cdusul= #cdusul#
     </isNotEmpty>
     <isNotEmpty prepend="AND" property="fseleg">
           eu.fseleg= #fseleg#
     </isNotEmpty>
  </select>

</sqlMap>