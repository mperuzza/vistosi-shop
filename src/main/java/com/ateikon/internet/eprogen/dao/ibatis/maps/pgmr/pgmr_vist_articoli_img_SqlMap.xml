  <select id="getSchedatec" parameterClass="map" resultClass="String">
      SELECT DISTINCT pathschtec
        FROM pgmr.vist_articoli_img,
             pgmr.mrp_arch_articoli,
             pgmr.mis_reparto
       WHERE vist_articoli_img.cdarti = mrp_arch_articoli.cdarti
         AND mrp_arch_articoli.cdrepa = mis_reparto.cdrepa
       <isNotEmpty property="cdvisttp" prepend="AND">
            mrp_arch_articoli.cdvisttp = #cdvisttp:VARCHAR#
       </isNotEmpty>
       <isNotEmpty property="cdvistfam" prepend="AND">
            mrp_arch_articoli.cdvistfam = #cdvistfam:VARCHAR#
       </isNotEmpty>
       <isNotEmpty property="cdvistv1" prepend="AND">
            mrp_arch_articoli.cdvistv1 = #cdvistv1:VARCHAR#
       </isNotEmpty>
       <isEmpty property="cdvistv1" prepend="AND">
            mrp_arch_articoli.cdvistv1 is null
       </isEmpty>
       <isNotEmpty property="cdvistv2" prepend="AND">
            mrp_arch_articoli.cdvistv2 = #cdvistv2:VARCHAR#
       </isNotEmpty>
       <isEmpty property="cdvistv2" prepend="AND">
            mrp_arch_articoli.cdvistv2 is null
       </isEmpty>
       <isNotEmpty property="cdvistv3" prepend="AND">
            mrp_arch_articoli.cdvistv3 = #cdvistv3:VARCHAR#
       </isNotEmpty>
       <isEmpty property="cdvistv3" prepend="AND">
            mrp_arch_articoli.cdvistv3 is null
       </isEmpty>
       <isNotEmpty property="cdvistelet" prepend="AND">
            mrp_arch_articoli.cdvistelet = #cdvistelet:VARCHAR#
       </isNotEmpty>
       <isNotEmpty prepend="AND" property="fgweb">
            mrp_arch_articoli.fgweb = #fgweb:VARCHAR#
            AND  mis_reparto.fgweb = #fgweb:VARCHAR#
       </isNotEmpty> 
       <isNotEmpty prepend="AND" property="cdrepa_escl_List">
            mis_reparto.cdrepa NOT IN
            <iterate close=")" conjunction="," open="(" property="cdrepa_escl_List">
                #cdrepa_escl_List[]#
            </iterate>
       </isNotEmpty>       
       AND pathschtec is not null
       ORDER by pathschtec
  </select>
