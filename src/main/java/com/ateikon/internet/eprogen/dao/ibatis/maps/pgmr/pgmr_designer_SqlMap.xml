  <select id="getProdotti" parameterClass="map" resultMap="designerProductResultMap">
    SELECT DISTINCT cdvisttp, cdvistfam
    FROM pgmr.mrp_arch_articoli,
      pgmr.mis_reparto
    WHERE  mrp_arch_articoli.cdrepa = mis_reparto.cdrepa
      AND  mrp_arch_articoli.fgweb = 'S'
      AND  mis_reparto.fgweb = 'S'
      AND cdvistfam is not null
      AND cdvisttp is not null
    <isNotEmpty property="coll_List" prepend="AND">
        mrp_arch_articoli.cdvistccol IN
        <iterate property="coll_List" open="(" close=")" conjunction="," >
            #coll_List[]#
        </iterate>
    </isNotEmpty>
      <!--AND cddesigner = #cddesigner:VARCHAR#-->
      AND cddesigner IN (
        SELECT cddesigner
          FROM pgmr.designer
         WHERE cddesigner = #cddesigner:VARCHAR#
            OR lista_designer LIKE '%' || #cddesigner:VARCHAR# || '%')
    ORDER BY cdvistfam
   </select>

  <select id="getArchivioDesigner" parameterClass="map" resultMap="ibatorgenerated_BaseResultMap">
    SELECT
    <include refid="pgmr_designer.ibatorgenerated_Base_Column_List" />
    ,
    <include refid="pgmr_designer.ibatorgenerated_Blob_Column_List" />
    FROM pgmr.designer
    <!--, pgmr.mrp_arch_articoli-->
   WHERE
   <!--designer.cddesigner = mrp_arch_articoli.cddesigner
     AND mrp_arch_articoli.cdclas_a IN ('L', 'UL')-->
     <![CDATA[ (designer.dsextdesigner <> null OR designer.dsextdesigner_eng <> null)]]>
    ORDER BY designer.dsdesigner
   </select>
