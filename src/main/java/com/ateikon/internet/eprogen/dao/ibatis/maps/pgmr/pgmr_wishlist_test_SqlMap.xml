    </isNotEmpty>
    <isNotEmpty prepend="AND" property="dtinse">
        <![CDATA[t.dtinse > #dtinse:TIMESTAMP#]]>
    </isNotEmpty>
    <isNotEmpty prepend="AND" property="key">
        AND (upper(c.ragsoc) like upper(#key:VARCHAR#) OR upper(c.cdcliente) like upper(#key:VARCHAR#) OR upper(t.dslist) like upper(#key:VARCHAR#))
