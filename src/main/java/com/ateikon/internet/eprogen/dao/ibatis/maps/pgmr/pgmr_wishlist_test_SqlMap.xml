  <select id="searchWishlist" parameterClass="java.util.Map" resultMap="searchResultMap">
 SELECT *
   FROM pgmr.wishlist_test t left join pgmr.cliente c on t.cdcliente=c.cdcliente
   <dynamic prepend="WHERE">
    <isNotEmpty prepend="AND" property="tkutente">
        t.tkutente = #tkutente:NUMERIC#
    </isNotEmpty>
    <isNotEmpty prepend="AND" property="tipo">
